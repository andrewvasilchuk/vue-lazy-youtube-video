{
  "name": "vue-lazy-youtube-video",
  "version": "2.3.0",
  "private": false,
  "description": "Vue.js component for lazyloading YouTube videos",
  "keywords": [
    "youtube",
    "youtube-video",
    "vue-youtube",
    "lazyload",
    "video-lazyload",
    "vue"
  ],
  "homepage": "https://github.com/3vilArthas/vue-lazy-youtube-video#readme",
  "bugs": {
    "url": "https://github.com/3vilArthas/vue-lazy-youtube-video/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3vilArthas/vue-lazy-youtube-video.git"
  },
  "license": "MIT",
  "author": "Andrew Vasilchuk <andrew.d.vasilchuk@gmail.com>",
  "files": [
    "src",
    "dist",
    "types/*.d.ts"
  ],
  "main": "dist/vue-lazy-youtube-video.cjs.js",
  "unpkg": "dist/vue-lazy-youtube-video.min.js",
  "module": "dist/vue-lazy-youtube-video.mjs",
  "types": "types/index.d.ts",
  "style": "dist/style.css",
  "scripts": {
    "build": "rimraf dist && vite -c vite.config.ts build && vite -c vite.config.minify.ts build && node ./build/style.js",
    "dev": "vite -c vite.config.ts demo",
    "prettier": "prettier --check ./**/*.{js,ts,vue}",
    "prettier:write": "prettier --write ./**/*.{js,ts,vue}",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:types": "vue-tsc --noEmit",
    "test": "npm run prettier && npm run test:types && npm run test:unit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@types/youtube": "0.0.47"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-typescript": "^11.1.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "cp-file": "^9.1.0",
    "cssnano": "^6.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "happy-dom": "^9.9.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.23",
    "postcss-custom-properties": "^13.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.4"
  },
  "peerDependencies": {
    "vue": "~3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}